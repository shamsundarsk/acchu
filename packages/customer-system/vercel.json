{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/client",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "api/upload-file.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "Cache-Control": "public, max-age=0, must-revalidate" },
      "dest": "/assets/$1"
    },
    {
      "src": "/api/test-job",
      "dest": "/api/test-job.js"
    },
    {
      "src": "/api/upload-file",
      "dest": "/api/upload-file.js"
    },
    {
      "src": "/api/download-file",
      "dest": "/api/download-file.js"
    },
    {
      "src": "/api/delete-files",
      "dest": "/api/delete-files.js"
    },
    {
      "src": "/api/print-jobs/pending",
      "dest": "/api/pending.js"
    },
    {
      "src": "/api/print-jobs/([^/]+)/cancel",
      "dest": "/api/cancel-job.js"
    },
    {
      "src": "/session/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

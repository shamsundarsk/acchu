{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install && npm run build:client",
  "outputDirectory": "dist/client",
  "routes": [
    {
      "src": "/api/sessions",
      "dest": "/api/sessions.js"
    },
    {
      "src": "/api/print-jobs/pending",
      "dest": "/api/pending.js"
    },
    {
      "src": "/session/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
